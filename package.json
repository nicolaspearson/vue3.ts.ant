    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jest-environment-jsdom": "26.6.2",
    "jest-serializer-vue": "^2.0.2",
    "node-sass": "^4.14.1",
    "npm-check-updates": "~11.2.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "sass-loader": "^10.0.2",
    "ts-jest": "^26.5.3",
