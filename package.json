{
  "name": "vue3.ts.ant",
  "version": "0.1.0",
  "description": "A Vue.js starter project, written using Typescript and Yarn2",
  "license": "MIT",
  "scripts": {
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lint:eslint": "eslint --cache",
    "lint:prettier": "prettier src && prettier tests && prettyhtml public && prettyhtml src",
